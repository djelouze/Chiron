<ServerManagerConfiguration>
   <ProxyGroup name="sources">
      <!-- ================================================================== -->
      <SourceProxy name="ChainSource"
                   class="vtkChainSource"
                   label="Chain Source">
         <Documentation
                       long_help="."
                       short_help="Output a chain (line or vertices)  from a vtkPoints.">
         </Documentation>
         <IntVectorProperty name="CellType"
                            label="Cell Type"
                            command="SetCellType"
                            number_of_elements="1"
                            default_values="0">
            <EnumerationDomain name="enum">
               <Entry value="0" text="Vertices"/>
               <Entry value="1" text="Polyline"/>
            </EnumerationDomain>
            <Documentation>
            Define the dimension of the output chain: 0D (vertices only) or 1D (polylines)
            </Documentation>
         </IntVectorProperty>
      </SourceProxy>
      <!-- End ChainSource -->
   </ProxyGroup>
   <!-- End Source Group -->
</ServerManagerConfiguration>
